class RecentCounter {
public:
vector<int>temp;
    RecentCounter() {
        
    }
    
    int ping(int t) {
        temp.push_back(t);
        int count=0;
        for(int i=temp.size()-1;i>=0;i--){
            if(t-temp[i]<=3000) count++;
            else break;
        }
        return count;
    }
};

/**
 * Your RecentCounter object will be instantiated and called as such:
 * RecentCounter* obj = new RecentCounter();
 * int param_1 = obj->ping(t);
 */
