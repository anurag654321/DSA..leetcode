class Solution {
public:
    string kthDistinct(vector<string>& arr, int k) {
        unordered_map<string,int>m;
        int c=0;
        for(auto it:arr){
            m[it]++;

        }
        for(auto i:arr){
            if(m[i]==1){
                c++;
                if(c==k) return i;
            }
           
        }
        return "";
    }
};
